
   <%= simple_form_for story do |f| %>
    <%= f.input :photos, as: :file, input_html: { multiple: true } %>
    <%= f.input :title %>
    <%= f.input :genre %>
    <%= f.association :category %>
    <%= f.input :synopsis %>
    <%= f.rich_text_area :rich_content %>
    <%= f.submit "Add a New Story", class: "btn btn-primary"%>
     <% end %>
